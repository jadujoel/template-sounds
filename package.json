{
  "name": "template-sounds",
  "scripts": {
    "serve": "echo 'use the provided host url together with ?ecas-load-path=<URL>/' && serve dist --cors",
    "build": "rm -rf dist && ecas-encoder-cli --config=config/index.ts --outfile=dist/config.json --logger=console --all=true && rm -f dist/soundsData.json",
    "watch": "node watch.mjs"
  },
  "devDependencies": {
    "@netent-tech/ecas-encoder-cli": "2.1.2-9bd72a63f3bbce546b12877c99c18ccb887be558.0",
    "@netent-tech/ecas-engine": "2.1.2",
    "chokidar": "3.5.3",
    "serve": "14.2.0",
    "typescript": "5.0.4"
  }
}
