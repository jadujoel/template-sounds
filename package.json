{
  "name": "template-sounds",
  "scripts": {
    "clean": "rm -rf dist .ecas-encoder",
    "serve": "node info.js && serve dist --cors",
    "dev": "npm run watch & npm run serve",
    "build": "ecas-encoder --config=config/index.ts --outfile=dist/config.json  --all=true --logger=console",
    "watch": "ecas-encoder --config=config/index.ts --outfile=dist/config.json  --all=false --logger=none --watch=config,sounds,dist/main"
  },
  "devDependencies": {
    "@netent-tech/ecas-engine": "2.1.3",
    "@netent-tech/ecas-encoder": "2.1.4-c96139ddc361c3b8f24863dbba99ea6c24c53587.0",
    "serve": "14.2.0",
    "typescript": "5.0.4"
  }
}
